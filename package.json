{
  "name": "bc-minecraft-molang",
  "version": "1.21.100-5",
  "description": "The library designed to deal with minecraft bedrock molang",
  "main": "./lib/src/main.js",
  "types": "./lib/src/main.d.ts",
  "scripts": {
    "compile": "tsc -b",
    "build": "npm run clean && npm run compile",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "test": "jest",
    "lint": "eslint",
    "clean": "rimraf lib",
    "pretest": "npm run compile",
    "prepublishOnly": "npm test && npm run compile",
    "preversion": "",
    "postversion": "git push && git push --tags",
    "version": "git add -A src",
    "watch": "tsc -w -p ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Blockception/BC-Minecraft-Molang.git"
  },
  "keywords": [
    "ts",
    "library",
    "minecraft",
    "molang"
  ],
  "author": "Blockception Ltd",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/Blockception/BC-Minecraft-Molang/issues"
  },
  "homepage": "https://github.com/Blockception/BC-Minecraft-Molang#readme",
  "files": [
    "lib/src/**/*.js",
    "lib/src/**/*.js.map",
    "lib/src/**/*.d.ts"
  ],
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "eslint": "^9.33.0",
    "eslint-plugin-jest": "^29.0.1",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1"
  },
  "dependencies": {
    "bc-minecraft-bedrock-types": "^1.22.1-13"
  }
